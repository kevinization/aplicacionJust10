<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoacti"
    tools:context=".activity_matematicas_ej3_16">

    <TextView
        android:id="@+id/textView201"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cuadrículas engañosas "
        android:textColor="#F44336"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView202"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fíjate cómo calcula Leti y ayúdale a terminar. "
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView201" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="409dp"
        android:layout_height="182dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView202"
        app:srcCompat="@drawable/leti" />

    <TextView
        android:id="@+id/textView203"
        android:layout_width="342dp"
        android:layout_height="38dp"
        android:layout_marginTop="8dp"
        android:text="Completa la multiplicación que representa el rectángulo de arriba: "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.231"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView53" />

    <TextView
        android:id="@+id/textView204"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="16"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/textView205"
        app:layout_constraintTop_toBottomOf="@+id/textView203" />

    <TextView
        android:id="@+id/textView206"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="12"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/textView207"
        app:layout_constraintTop_toBottomOf="@+id/textView203" />

    <TextView
        android:id="@+id/textView205"
        android:layout_width="27dp"
        android:layout_height="24dp"
        android:layout_marginTop="21dp"
        android:text="x"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/textView206"
        app:layout_constraintTop_toBottomOf="@+id/textView203" />

    <TextView
        android:id="@+id/textView207"
        android:layout_width="27dp"
        android:layout_height="24dp"
        android:layout_marginTop="21dp"
        android:text="="
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/mat316_txtNum1"
        app:layout_constraintTop_toBottomOf="@+id/textView203" />

    <EditText
        android:id="@+id/mat316_txtNum1"
        android:layout_width="72dp"
        android:layout_height="32dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="216dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView203" />

    <TextView
        android:id="@+id/textView208"
        android:layout_width="348dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:text="Resuelve las siguientes multiplicaciones. Utiliza el procedimiento de Leti. "
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mat316_txtNum1" />

    <ImageView
        android:id="@+id/imageView54"
        android:layout_width="319dp"
        android:layout_height="181dp"
        android:layout_marginTop="8dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView208"
        app:srcCompat="@drawable/letidos" />

    <EditText
        android:id="@+id/mat316_txtNum2"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="59dp"
        android:ems="10"
        android:enabled="false"
        android:hint="15"
        android:inputType="number"
        android:text="15"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView54"
        app:layout_constraintTop_toTopOf="@+id/imageView54" />

    <EditText
        android:id="@+id/mat316_txtNum3"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginEnd="59dp"
        android:ems="10"
        android:enabled="false"
        android:hint="18"
        android:inputType="number"
        android:text="18"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView54"
        app:layout_constraintTop_toBottomOf="@+id/mat316_txtNum2" />

    <EditText
        android:id="@+id/mat316_txtNum4"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="59dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView54"
        app:layout_constraintTop_toBottomOf="@+id/mat316_txtNum3" />

    <EditText
        android:id="@+id/mat316_txtNum5"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginEnd="59dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView54"
        app:layout_constraintTop_toBottomOf="@+id/mat316_txtNum4" />

    <EditText
        android:id="@+id/mat316_txtNum6"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="59dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView54"
        app:layout_constraintTop_toBottomOf="@+id/mat316_txtNum5" />

    <TextView
        android:id="@+id/mat316_txtAciertos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="4dp"
        android:onClick="comprobacion"
        android:text="Aciertos:"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:onClick="comprobacion"
        android:text="Continuar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>